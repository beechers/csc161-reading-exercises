// In simpleloops.c
// Compile with -std=c11 flag so that variable-initialization in loops works

#include <stdio.h>

int main(void) {
    for (int i = 0; i < 10; i++) {
        printf("%d", i);
    }
    printf("\n");
    for (int i = 0; i <= 10; i++) {
        printf("%d", i);
    }
    printf("\n");
    for (int i = 1; i <= 10; i++) {
        printf("%d", i);
    }
    printf("\n");
    for (int i = 10; i > 0; i--) {
        printf("%d", i);
    }
     printf("\n");
    for (int i = 10; i >= 0; i--) {
        printf("%d", i);
    }
printf("\n");
    return 0;
}
